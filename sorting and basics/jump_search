import math 
  
def jmpFind( arr , x , n ):  
    step = math.sqrt(n) 
    prev = 0
    while arr[int(min(step, n)-1)] < x: 
        prev = step 
        step += math.sqrt(n) 
        if prev >= n: 
            return -1
    while arr[int(prev)] < x: 
        prev += 1
          
        if prev == min(step, n): 
            return -1    #element not found
    if arr[int(prev)] == x: 
        return prev 
      
    return -1
